<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gek.learnwords">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_louncher_lw"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.WordActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name"/>

        <activity android:name=".activity.ListWordsActivity"
            android:screenOrientation="portrait"
            android:label="@string/caption_list_word"
            android:theme="@style/AppThemeNoActionBar"/>

        <activity android:name=".activity.IEActivity"
            android:screenOrientation="portrait"
            android:label="@string/caption_ie"/>

        <activity android:name=".activity.LearnActivity"
            android:screenOrientation="portrait"
            android:label="@string/caption_learn"/>

        <activity android:name=".activity.TestActivity"
            android:screenOrientation="portrait"
            android:label="@string/caption_test"/>

        <activity android:name=".activity.SettingsActivity"
            android:screenOrientation="portrait"
            android:label="@string/caption_settings"/>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>